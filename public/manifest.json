{
  "manifest_version": 3,
  "name": "Emosync for Students",
  "description": "Vite Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.svg"
  },

  "icons": {
    "72": "icon-72x72.png",
    "96": "icon-72x72.png",
    "128": "icon-72x72.png"
  },
  "permissions": ["storage", "scripting", "tabs", "activeTab"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "key": ""
}
