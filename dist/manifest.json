{
  "manifest_version": 3,
  "name": "Emodu v2 Extension",
  "version": "1.0",
  "key": "",
  "description": "Chrome Extension untuk Google Meet menggunakan React, Vite, dan JavaScript",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "72": "icon-72x72.png",
      "96": "icon-96x96.png",
      "128": "icon-128x128.png"
    }
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "content.js-loader.js"
      ],
      "matches": [
        "https://meet.google.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "555.wav",
        "inject.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "resources": [
        "constants.js",
        "content.js.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
